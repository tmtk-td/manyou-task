<p>
  <%= "名前" %>:<%= @user.name %>
</p>
<p><%= "メール" %>:<%= @user.email %>
</p>
<p>
  <br>
  <table align="center">
    <tr>
    
      <th>Name</th>
      <th>Detail</th>
      <th>Deadline</th>
      <th>Completed</th>
      <th>Priority</th>
    </tr>
    <% @tasks.each do |task| %>
      <tr class = "task_row">
        <td><%= task.name %></td>
        <td><%= task.detail %></td>
        <td><%= task.deadline %></td>
        <td><%= task.completed %></td>
        <td><%= task.priority %></td>
      </tr>
    <% end %>
  </table>
</p>
